  "scripts": {
    "start": "react-app-rewired start",
    "build": "BUILD_PATH=../app/static react-app-rewired build",
    "test": "react-app-rewired test",
    "test:functional": "if curl -sfo /dev/null localhost:3000; then yarn cypress:run; else echo 'Error: are React dev-server (port 3000) and flask app (port 5000) both running?'; exit -1; fi",
    "lint": "yarn run eslint src/",
    "format": "yarn prettier --write .",
    "prepare": "cd ../.. && husky install webapp/client/.husky",
    "storybook": "start-storybook -p 6006 -s public",
    "storybook:build": "build-storybook -s public",
    "cypress": "cypress open",
    "cypress:run": "cypress run"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
